integer main()
{
	integer childpid, retval;
	childpid = Fork();
	if(childpid == -2) then
		retval = Exec("sample.xsm");
	else
		while(childpid != -1) do
		retval = Getpid();
		retval= Getppid();
		if(retval==18)then
			breakpoint;
		endif;
//		if(childpid==19)then
//			breakpoint;
//		endif;
//			print(childpid);
		endwhile;
	endif;
return 0;
}
