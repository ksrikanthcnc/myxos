decl
	integer status,index,lseek;
	string name,word,num;
enddecl
integer main()
{
	print("-----------------");	
	print("1.create");
	print("2.delete");
	print("3.open");
	print("4.close");
	print("5.read");
	print("6.seek");
	print("7.write");
	print("8.//breakpoint");
	print("9.ls");
	print("0.stop");
	print("Type");

	while(1==1)do	
	print("-----------------");	

	read(num);
	
	if(num=="stop")then
	break;
	endif;

	if(num=="create")then
	print("name:");
	read(name);
	status = Create(name);
	print(status);
	//breakpoint;
	endif;

	if(num=="delete")then
	print("name:");
	read(name);
	status = Delete(name);
	print(status);
	//breakpoint;
	endif;

	if(num=="open")then
	print("name:");
	read(name);
	status = Open(name);
	print(status);
	//breakpoint;
	endif;

	if(num=="close")then
	print("index:");
	read(index);
	status = Close(index);
	print(status);
	//breakpoint;
	endif;

	if(num=="read")then
	print("index:");
	read(index);
	status = Read(index,word);
	print(word);
	print(status);
	//breakpoint;
	endif;

	if(num=="lseek")then
	print("index");
	read(index);
	print("lseek:");
	read(lseek);
	status = Seek(index,lseek);
	print(status);
	//breakpoint;
	endif;

	if(num=="write")then
	print("index");
	read(index);
	print("word:");
	read(word);
	status = Write(index,word);
	print(status);
	//breakpoint;
	endif;

	if(num=="ls")then
	status = Open("");
	print(word);
	print(status);
	//breakpoint;
	endif;

//	if(num==8)then
	//breakpoint;
//	endif;

	endwhile;
	return 0;
}
