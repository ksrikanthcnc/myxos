integer main()
{
	integer status,temp;
	string file;
	while(1==1)do
		print("-----------------");
		read(file);
		if(file=="help"||file=="ls")then
			file="ls.xsm";
		endif;
		if(file=="breakpoint")then
			breakpoint;
		endif;
		status=Fork();
		if(status==-2)then
			temp=Exec(file);
			if(temp==-1)then
				print("no-file");
				temp=Exec("ls.xsm");
				Exit();
			endif;
		endif;
		if(status!=-2)then
			temp=Wait(status);
		endif;
	endwhile;
return 0;
}
